{
  "expo": {
    "name": "myFirstApp",
    "slug": "myFirstApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/image2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/op.png",
      "resizeMode": "cover",
      "backgroundColor": "#18302D"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/image2.png",
        "backgroundColor": "#18302D"
      }
    },
    "scheme": "exponavigation",
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}
