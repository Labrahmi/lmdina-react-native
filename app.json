{
  "expo": {
    "name": "myFirstApp",
    "slug": "myFirstApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/3244411-ai.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/splash-new.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "scheme": "exponavigation",
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
